{
  "name": "webp-jxr-middleware",
  "version": "1.0.2",
  "description": "Express middleware to replace jpg/png/jpeg with webp/jxr when supported",
  "main": "webp-jxr-middleware.js",
  "scripts": {
    "prepublish": "npm run es5",
    "test": "./node_modules/.bin/babel-node  ./node_modules/.bin/babel-istanbul cover test/*.spec.js | tap-summary",
    "dev": "DEBUG=express:* ./node_modules/.bin/babel-node src/dev",
    "lint": "./node_modules/.bin/eslint src/*.js test/*.js",
    "docs": " ./node_modules/.bin/jsdoc --pedantic -c .jsdoc src --readme README.md",
    "es5": "./node_modules/.bin/babel src/index.js --out-file webp-jxr-middleware.es5.js"
  },
  "commitMsg": {
    "bodyMaxLineLength": {
      "length": 100
    },
    "references": {
      "github": false
    },
    "strictTypes": false,
    "subjectMaxLineLength": {
      "length": 50
    },
    "types": {
      "allowedTypes": "^(feat|fix|docs|style|refactor|perf|test|chore)(\\([^)]+\\))?: ",
      "required": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "precommit": [
    "npm run docs",
    "npm run lint",
    "npm run test",
    "npm run es5"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obartra/webp-jxr-middleware.git"
  },
  "files": [
    "README.md",
    "webp-jxr-middleware.js",
    "webp-jxr-middleware.es5.js"
  ],
  "keywords": [
    "webp",
    "jxr",
    "express",
    "connect",
    "middleware",
    "image"
  ],
  "author": "Oscar Bartra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/obartra/webp-jxr-middleware/issues"
  },
  "homepage": "https://github.com/obartra/webp-jxr-middleware#readme",
  "dependencies": {
    "vary": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.13.2",
    "blue-tape": "^0.2.0",
    "cz-conventional-changelog": "^1.1.7",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "express": "^4.14.0",
    "jsdoc": "^3.4.0",
    "precommit": "^1.2.2",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0",
    "tap-summary": "^3.0.1"
  }
}
